{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "importOrder": [
    "^next/(.*)$",
    "^@/components/(.*)$",
    "^react$",
    "^@/utils$",
    "~images/(.*)$",
    "@/styles/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
